{
    "settings": {
        "number_of_shards": 1
    },
    "mappings": {
        "date_detection": false,
        "dynamic_templates": [
            {
                "strings": {
                    "match": "*",
                    "match_mapping_type": "string",
                    "mapping": {
                        "type": "keyword"
                    }
                }
            }
        ],
        "_source": {
            "enabled": true
        },
        "properties": {
            "Timestamp": {
                "type": "date"
            },
            "MetricValue": {
                "type": "double"
            }
        }
    }
}